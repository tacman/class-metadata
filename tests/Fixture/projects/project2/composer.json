{
    "require": {
        "zenstruck/class-metadata": "@dev",
        "zenstruck/assert": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "extra": {
        "class-metadata": {
            "paths": false,
            "map": {
                "Zenstruck\\Assert": "assert",
                "Zenstruck\\Assert\\Expectation": {
                    "alias": "expectation",
                    "key1": "value",
                    "key2": 7
                }
            }
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../../../../"
        }
    ],
    "config": {
        "allow-plugins": {
            "zenstruck/class-metadata": true
        }
    }
}
